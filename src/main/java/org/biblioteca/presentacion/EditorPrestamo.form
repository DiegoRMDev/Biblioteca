<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="org.biblioteca.presentacion.EditorPrestamo">
  <grid id="27dc6" binding="mainPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="5" y="5" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <grid id="3ad80" binding="panelSuperior" layout-manager="GridLayoutManager" row-count="2" column-count="2" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
        <margin top="0" left="0" bottom="0" right="0"/>
        <constraints border-constraint="North"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="7e92" class="javax.swing.JLabel" binding="lblLector">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="Lector"/>
            </properties>
          </component>
          <component id="7f1cd" class="javax.swing.JComboBox" binding="cboLectores">
            <constraints>
              <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="2" anchor="8" fill="1" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <model/>
            </properties>
          </component>
          <component id="8fb7a" class="javax.swing.JLabel" binding="lblFechaDev">
            <constraints>
              <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="Fecha Devolución Prevista:"/>
            </properties>
          </component>
          <component id="eb781" class="javax.swing.JSpinner" binding="spinnerFechaDevolucion">
            <constraints>
              <grid row="1" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties/>
          </component>
        </children>
      </grid>
      <grid id="c55eb" binding="panelCentral" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <grid id="7a86a" binding="panelAgregarLibro" layout-manager="GridLayoutManager" row-count="2" column-count="2" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
            <margin top="0" left="0" bottom="0" right="0"/>
            <constraints border-constraint="West"/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="a09f1" class="javax.swing.JLabel" binding="Libro">
                <constraints>
                  <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <text value="Libro:"/>
                </properties>
              </component>
              <component id="3fa57" class="javax.swing.JComboBox" binding="cboLibros">
                <constraints>
                  <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="2" anchor="8" fill="1" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties/>
              </component>
              <component id="5fadd" class="javax.swing.JLabel" binding="lblCantidad">
                <constraints>
                  <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <text value="Cant:"/>
                </properties>
              </component>
              <component id="91570" class="javax.swing.JSpinner" binding="spinnerCantidad">
                <constraints>
                  <grid row="1" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties/>
              </component>
            </children>
          </grid>
          <scrollpane id="4ed58" binding="scrollDetalles">
            <constraints border-constraint="Center"/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="b981c" class="javax.swing.JTable" binding="tablaDetalles" custom-create="true">
                <constraints/>
                <properties/>
              </component>
            </children>
          </scrollpane>
        </children>
      </grid>
      <grid id="744d0" binding="panelInferior" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
        <constraints border-constraint="South"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="40cd6" class="javax.swing.JButton" binding="btnGuardar">
            <constraints/>
            <properties>
              <text value="Guardar Préstamo"/>
            </properties>
          </component>
          <component id="afcdc" class="javax.swing.JButton" binding="btnCancelar">
            <constraints/>
            <properties>
              <text value="Cancelar"/>
            </properties>
          </component>
        </children>
      </grid>
      <grid id="6850f" binding="panelQuitar" layout-manager="GridLayoutManager" row-count="2" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
        <margin top="0" left="0" bottom="0" right="0"/>
        <constraints border-constraint="East"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="4c014" class="javax.swing.JButton" binding="btnQuitarLibro">
            <constraints>
              <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="Quitar Libro"/>
            </properties>
          </component>
          <component id="e46ca" class="javax.swing.JButton" binding="btnAgregarLibro">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="Agregar Libro"/>
            </properties>
          </component>
        </children>
      </grid>
    </children>
